<h1>Projects#new</h1>
<p>Find me in app/views/projects/new.html.erb</p>

<%= form_for @project do |f| %>
  <p>
    <%= f.label "Project Name" %><br>
    <%= f.text_field "name" %>
  </p>
  <p>
    <%= f.label "Funding Goal" %><br>
    <%= f.number_field "funding_goal" %>
  </p>
  <p>
    <%= f.label "Start Date" %><br>
    <%= f.date_field "start_date" %>
  </p>
   <p>
    <%= f.label "End Date" %><br>
    <%= f.date_field "end_date" %>
  </p>
   <p>
    <%= f.label "Description" %><br>
    <%= f.text_field "description" %>
  </p>
   <p>
    <%= f.label "Photo" %><br>
    <%= f.url_field "photo" %>
  </p>
  <p>
    <%= f.submit "Save" %>
  </p>
<% end %>
